<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lifecycleactivity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LifecycleActivity">
        <activity android:name=".cart"></activity>
        <activity android:name=".nonveg" />
        <activity android:name=".ice_creams" />
        <activity android:name=".beverages" />
        <activity android:name=".meals" />
        <activity android:name=".breakfast" />
        <activity android:name=".snack" />
        <activity android:name=".menu" />
        <activity
            android:name=".snack_page"
            android:label="@string/title_activity_snack_page"
            android:theme="@style/Theme.LifecycleActivity.NoActionBar" />
        <activity
            android:name=".MainActivity2"
            android:label="@string/title_activity_main2"
            android:theme="@style/Theme.LifecycleActivity.NoActionBar" />
        <activity
            android:name=".snacks_page"
            android:label="@string/title_activity_snacks_page"
            android:theme="@style/Theme.LifecycleActivity.NoActionBar" />
        <activity
            android:name=".menu_page"
            android:label="@string/title_activity_menu_page"
            android:theme="@style/Theme.LifecycleActivity.NoActionBar" />
        <activity android:name=".MenuPage" />
        <activity android:name=".signup_page" />
        <activity android:name=".signin_page" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>